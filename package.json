{
  "name": "connexus",
  "author": "Terry Tan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openapi:yaml": "typeconv -f jsc -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'pages/api' 'prisma/generated/json-schema/json-schema.json'",
    "prisma:push": "yarn prisma db push --force-reset",
    "prisma:seed": "yarn prisma db seed",
    "prisma:generate": "yarn prisma generate",
    "prisma:db": "yarn prisma studio",
    "db:init": "yarn prisma:push && yarn prisma:seed && yarn prisma:generate && yarn openapi:yaml && yarn prisma:db",
    "postinstall": "prisma generate"
  },
  "dependencies": {
